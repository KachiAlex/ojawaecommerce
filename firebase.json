            },
            {
              "key": "Cache-Control",
              "value": "public, max-age=3600, must-revalidate"
            }
          ]
        },
        {
          "source": "robots.txt",
          "headers": [
            {
              "key": "Content-Type",
              "value": "text/plain"
            },
            {
              "key": "Cache-Control",
              "value": "public, max-age=86400"
            }
          ]
        }
      ],
      "cleanUrls": true,
      "trailingSlash": false
    }
  ],
  "emulators": {
    "functions": {
      "host": "127.0.0.1",
      "port": 5005
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5004
    },
    "ui": {
      "enabled": true,
      "host": "127.0.0.1",
      "port": 4002
    }
  },
  "functions": [
    {
    "source": "functions",
      "codebase": "default",
    "runtime": "nodejs20",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.local"
    ]
    },
    {
      "source": "functions-routes",
      "codebase": "routes",
      "runtime": "nodejs20"
  }
  ]
}
